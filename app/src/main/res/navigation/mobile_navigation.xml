<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.npkadvisorfinal.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.npkadvisorfinal.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.npkadvisorfinal.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />

    <fragment
        android:id="@+id/cropsMenu"
        android:name="com.example.npkadvisorfinal.CropsMenu"
        android:label="Mis Cultivos"
        tools:layout="@layout/fragment_crops_menu" />
    <activity
        android:id="@+id/grafic"
        android:name="com.example.npkadvisorfinal.Grafic"
        android:label="activity_grafic"
        tools:layout="@layout/activity_grafic" />
    <activity
        android:id="@+id/mapas"
        android:name="com.example.npkadvisorfinal.Mapas"
        android:label="activity_mapas"
